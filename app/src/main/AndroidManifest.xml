<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dapp.librarymanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_iconlapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_iconlapp_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Activity.AddBook"></activity>
        <activity android:name=".Activity.Activity.MyBooks" />
        <activity android:name=".Activity.Activity.RegisterActivity" />
        <activity
            android:name=".Activity.Activity.MainActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.Activity.DescriptonActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait" />
        <activity android:name=".Activity.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>